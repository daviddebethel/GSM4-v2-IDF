# Release profile defaults.
CONFIG_GSM4_BUILD_PROFILE_RELEASE=y
CONFIG_GSM4_TEST_HARNESS_MODE=n
CONFIG_GSM4_SERIAL_DEBUG_ENABLED=n
CONFIG_GSM4_RELEASE_SAFE_DEFAULTS=y
CONFIG_GSM4_SERIAL_DEBUG_LOCKDOWN=y
CONFIG_GSM4_REQUIRE_RELEASE_SECURITY_CONTROLS=y

CONFIG_LOG_DEFAULT_LEVEL_WARN=y
CONFIG_LOG_DEFAULT_LEVEL=2
CONFIG_LOG_MAXIMUM_LEVEL_WARN=y
CONFIG_LOG_MAXIMUM_LEVEL=2
